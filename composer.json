{
    "name": "pantheon-upstreams/drupal-recommended",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": {
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "1": {
            "type": "path",
            "url": "upstream-configuration"
        },
        "2": {
            "type": "git",
            "url": "https://github.com/mautic/FOSOAuthServerBundle.git"
        }
    },
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.1",
        "drupal/bootstrap": "^3.24",
        "drupal/bootstrap4": "^2.1",
        "drupal/captcha": "^1.4",
        "drupal/colorbox": "^1.10",
        "drupal/commerce": "^2.35",
        "drupal/commerce_license": "^2.0@beta",
        "drupal/commerce_recurring": "^1.0@RC",
        "drupal/commerce_stripe": "^1.0",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-recommended": "^9.4",
        "drupal/entity_browser": "^2.6",
        "drupal/feeds": "^3.0@beta",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.2",
        "drupal/google_analytics": "^4.0",
        "drupal/graphql": "^4.5",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/insert_block": "^1.0@beta",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_operation_link": "^2.1",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/layout_custom_section_classes": "^1.0",
        "drupal/lightning_core": "^5.10",
        "drupal/lightning_media": "^4.6",
        "drupal/menu_block": "^1.8",
        "drupal/owlcarousel": "^2.0@beta",
        "drupal/paragraphs": "^1.15",
        "drupal/quicktabs": "^3.0@alpha",
        "drupal/restui": "^1.21",
        "drupal/shortcode": "^2.0",
        "drupal/social_auth": "^3.1",
        "drupal/social_post": "^3.0",
        "drupal/tb_megamenu": "^1.7",
        "drupal/twig_tweak": "^3.1",
        "drupal/views_accordion": "^2.0",
        "drupal/views_dates": "^1.1",
        "drupal/views_menu_children_filter": "^1.0@RC",
        "drupal/webform": "^6.1",
        "drupal/yoast_seo": "^1.7",
        "drush/drush": "^10",
        "pantheon-systems/drupal-integrations": "^9",
        "pantheon-upstreams/upstream-configuration": "self.version",
        "mautic/core-composer-scaffold": "4.x-dev",
        "mautic/core-project-message": "4.x-dev",
        "mautic/core-lib": "4.4.8",
        "mautic/grapes-js-builder-bundle": "4.4.8",
        "mautic/plugin-citrix": "4.4.8",
        "mautic/plugin-clearbit": "4.4.8",
        "mautic/plugin-cloudstorage": "4.4.8",
        "mautic/plugin-crm": "4.4.8",
        "mautic/plugin-emailmarketing": "4.4.8",
        "mautic/plugin-focus": "4.4.8",
        "mautic/plugin-fullcontact": "4.4.8",
        "mautic/plugin-gmail": "4.4.8",
        "mautic/plugin-outlook": "4.4.8",
        "mautic/plugin-social": "4.4.8",
        "mautic/plugin-tagmanager": "4.4.8",
        "mautic/plugin-zapier": "4.4.8",
        "mautic/theme-aurora": "4.4.8",
        "mautic/theme-blank": "4.4.8",
        "mautic/theme-brienz": "4.4.8",
        "mautic/theme-cards": "4.4.8",
        "mautic/theme-coffee": "4.4.8",
        "mautic/theme-confirmme": "4.4.8",
        "mautic/theme-fresh-center": "4.4.8",
        "mautic/theme-fresh-fixed": "4.4.8",
        "mautic/theme-fresh-left": "4.4.8",
        "mautic/theme-fresh-wide": "4.4.8",
        "mautic/theme-goldstar": "4.4.8",
        "mautic/theme-mauve": "4.4.8",
        "mautic/theme-nature": "4.4.8",
        "mautic/theme-neopolitan": "4.4.8",
        "mautic/theme-oxygen": "4.4.8",
        "mautic/theme-paprika": "4.4.8",
        "mautic/theme-skyline": "4.4.8",
        "mautic/theme-sparse": "4.4.8",
        "mautic/theme-sunday": "4.4.8",
        "mautic/theme-vibrant": "4.4.8",
        "mautic/theme-trulypersonal": "4.4.8"
    },
    "require-dev": {
        "drupal/core-dev": "^9.4",
        "drupal/devel": "^4.1"
    },
    "conflict": {
        "drupal/drupal": "*",
        "mautic/mautic": "*",
        "mautic/core": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ],
            "web/mautic/app": [
                "type:mautic-core"
              ],
              "web/mautic/plugins/{$name}": [
                "type:mautic-plugin"
              ],
              "web/mautic/themes/{$name}": [
                "type:mautic-theme"
              ]
        },
        "mautic-scaffold": {
            "locations": {
                "web-root": "web/mautic/"
            }
        },
        "mautic-core-project-message": {
          "include-keys": [
            "homepage",
            "support"
          ],
          "post-create-project-cmd-message": [
            "<bg=blue;fg=white>                                                         </>",
            "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Mautic codebase  </>",
            "<bg=blue;fg=white>  from the mautic/recommended-project template!          </>",
            "<bg=blue;fg=white>                                                         </>",
            "",
            "<bg=yellow;fg=black>Next steps</>:",
            "  * Install Mautic",
            "  * Read the user guide",
            "  * Get support: https://www.mautic.org/support",
            "  * Get involved with the Mautic community:",
            "      https://www.mautic.org/getting-involved",
            "  * Remove the plugin that prints this message:",
            "      composer remove mautic/core-project-message"
          ]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "symfony/flex": true,
            "mautic/core-composer-scaffold": true,
            "mautic/core-project-message": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
      "psr-4": {
        "MauticPlugin\\": "web/mautic/plugins/"
      }
    }
}
